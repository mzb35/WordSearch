{
  "name": "WordSearch",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts":[
    {
      "matches":["<all_urls>"],
      "js": ["wordsearch.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "permissions" : ["storage", "activeTab", "scripting"]
}